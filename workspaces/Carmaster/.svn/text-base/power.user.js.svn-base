    sendSecondReport: function(){
    	var t = Tabs.Crest;
    	if(!Options.crestreport || !CrestOptions.Running) return;
    	var now = new Date().getTime()/1000.0;
        now = now.toFixed(0);
        if (now < (parseInt(Options.lastCrestReport)+(Options.CrestMsgInterval*60*60))) return;
    	
    	var total = 0;
    	var wildtype = '';
