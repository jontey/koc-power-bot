  toggleAutoTrainState: function(obj){
	var t = Tabs.AutoTrain;
	obj = document.getElementById('pbAutoTrainState');
    if (TrainOptions.Running == true) {
        TrainOptions.Running = false;
        obj.value = "AutoTrain = OFF";
